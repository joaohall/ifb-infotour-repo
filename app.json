{ 
  "expo": {
    "name": "InfoTour",
    "slug": "InfoTour",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon1.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splashart1.png",
      "resizeMode": "contain",
      "backgroundColor": "#AC58AA",
      "privacy": "public"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "andoid-permissions":{
        "permissions" : "CAMERA"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
